    $(document).ready(function() {
        $.get('./content/_favoritethings.txt', function (data) {
            var quotes = data.split(";");
            var idx = Math.floor(quotes.length * Math.random());
            $('.favoritethings-yes').html(quotes[idx]);
        });
    });
